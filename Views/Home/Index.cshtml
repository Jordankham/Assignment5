@{
    ViewData["Title"] = "Home Page";
}
@model IQueryable<Bookstore>

<header>
    <h1>Bookstore</h1>
</header>

@foreach (var x in Model)
{
<div>
    <h5>Book Title: @x.Title</h5>
    <p> Author Name: @x.AuthorFname @x.AuthorLname</p>
    <p>Publisher: @x.Publisher </p>
    <p>ISBN: @x.ISBN </p>
    <p>Classification: @x.Classification </p>
    <p>Category: @x.Category</p>
    <p>Price: $@x.Price </p>
</div>
}